{  "swagger": "2.0",  "info": {    "version": "1.0.1",    "title": "neutrino-validation",    "description": "API for email validation",    "termsOfService": "http://swagger.io/terms/",    "contact": {      "name": "Swagger API Team",      "email": "foo@example.com",      "url": "http://madskristensen.net"    },    "license": {      "name": "MIT",      "url": "http://github.com/gruntjs/grunt/blob/master/LICENSE-MIT"    }  },  "host": "https://community-neutrino-email-validate.p.mashape.com",  "basePath": "",  "schemes": [    "http"  ],  "consumes": [    "application/json"  ],  "produces": [    "application/json"  ],  "paths": {    "/email-validate": {      "post": {        "description": "validates email",        "operationId": "validateEmail",        "parameters": [          {            "name": "email",            "in": "body",            "description": "email to validate",            "required": true,            "schema": {              "$ref": "#/definitions/Email"            }          }        ],        "responses": {          "200": {            "description": "email validation result",            "schema": {              "$ref": "#/definitions/EmailValidationResult"            }          },          "default": {            "description": "unexpected error",            "schema": {              "$ref": "#/definitions/Error"            }          }        }      }    }  },  "definitions": {    "Email": {      "required": [        "email"      ],      "properties": {        "email": {          "type": "string"        }      }    },    "EmailValidationResult": {      "required": [        "valid",        "email"      ],      "properties": {        "email": {          "type": "string"        },        "valid": {          "type": "boolean"        }      }    },    "Error": {      "required": [        "code",        "message"      ],      "properties": {        "code": {          "type": "integer",          "format": "int32"        },        "message": {          "type": "string"        }      }    }  }}